default2:Acrostic a owl:Class ;
    rdfs:label "Acrostic"@en ;
    rdfs:comment "Grouping of the first letter of each line that form a word, the alphabet or any other message. This class also includes telestichs, that is, the message create it with the last letter of each line"@en ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-literaryAnalysisElements/"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty default2:content ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        <http://iflastandards.info/ns/fr/frbr/frbroo/F2_Expression> .

default3:clausula a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "clausula"@en ;
    rdfs:comment "The categorisation of the item according to the rhythmic ending of a line  determined by the number of unstressed syllables following the last stressed one. This property is both valid for accentual and syllabic versification, so possible values include: masculine (zero syllables), femenine (one), dactylic, proparoxitone, etc."@en ;
    rdfs:domain default3:Foot ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements#clausula"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/clausula> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:clausulaSchemeType a owl:FunctionalProperty,
