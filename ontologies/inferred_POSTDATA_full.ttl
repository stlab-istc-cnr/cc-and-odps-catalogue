default3:MetricalEncoding a owl:Class ;
    rdfs:label "MetricalEncoding"@en ;
    ns1:creator "mluisadiez" ;
    ns1:date "2018-06-16T18:37:24+00:00"^^xsd:dateTime ;
    rdfs:comment """The notation employed to represent a metrical pattern.
Eg.: The definition of the plus sign to encode strong positions."""@en ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:metricalNotes ],
        owl:Thing .

default3:StanzaPattern a owl:Class ;
    rdfs:label "StanzaPattern"@en ;
    ns1:creator "mluisadiez" ;
    ns1:date "2018-06-16T18:37:24+00:00"^^xsd:dateTime ;
    rdfs:comment """The prosodic analysis using a Stanza as the unit of reference
Eg.: The number of lines of a Stanza or its categorisation according to metrics (such as “quatrain”)"""@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass [ a owl:Class ;
                    owl:intersectionOf ( ns4:Concept [ a owl:Restriction ;
                                owl:hasValue <http://postdata.linhd.uned.es/kos/clausulaScheme-type> ;
                                owl:onProperty ns4:inScheme ] ) ] ;
            owl:onProperty default3:clausulaSchemeType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass [ a owl:Class ;
                    owl:intersectionOf ( ns4:Concept [ a owl:Restriction ;
                                owl:hasValue <http://postdata.linhd.uned.es/kos/feet-type> ;
                                owl:onProperty ns4:inScheme ] ) ] ;
            owl:onProperty default3:feetType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass [ a owl:Class ;
                    owl:intersectionOf ( ns4:Concept [ a owl:Restriction ;
                                owl:hasValue <http://postdata.linhd.uned.es/kos/metrical-type> ;
                                owl:onProperty ns4:inScheme ] ) ] ;
            owl:onProperty default3:metricalType ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onClass [ a owl:Class ;
                    owl:intersectionOf ( ns4:Concept [ a owl:Restriction ;
                                owl:hasValue <http://postdata.linhd.uned.es/kos/rhymeMatching-type> ;
                                owl:onProperty ns4:inScheme ] ) ] ;
            owl:onProperty default3:presentsRhymeMatching ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:altCountingMetricalScheme ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:altPatterningMetricalScheme ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:altRhymeScheme ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:countingMetricalScheme ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty default3:hasCaesura ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:metricalNotes ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:patterningMetricalScheme ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty default3:rhymeScheme ],
        default3:Pattern .

