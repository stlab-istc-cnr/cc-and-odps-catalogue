            owl:allValuesFrom default1:DateEntity ;
            owl:onProperty :date ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Redaction ;
            owl:onProperty :isMentionedIn ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty :eventNarration ],
        [ a owl:Restriction ;
            owl:allValuesFrom default1:DateEntity ;
            owl:onProperty :date ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Redaction ;
            owl:onProperty :isMentionedIn ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns1:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom default1:DateEntity ;
            owl:onProperty :date ],
        [ a owl:Restriction ;
            owl:allValuesFrom :Redaction ;
            owl:onProperty :isMentionedIn ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onDataRange xsd:string ;
            owl:onProperty ns1:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom default1:DateEntity ;
