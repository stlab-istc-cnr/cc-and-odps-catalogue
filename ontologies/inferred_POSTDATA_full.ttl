    rdfs:label "clausulaSchemeType"@en ;
    rdfs:comment "Categorisation of the stanza according to the distribution of clausulae/gender of rhymes (eg. “classic”, “heterogonique”, etc.)"@en ;
    rdfs:domain default3:StanzaPattern ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/clausulaScheme-type> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

