default3:metricalComplexity a owl:ObjectProperty ;
    rdfs:label "metricalComplexity"@en ;
    rdfs:comment "Categorisation depending on the number of syllables: usually, less or more than eight"@en ;
    rdfs:domain default3:WorkPattern ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/metrical-complexity> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:metricalContext a owl:ObjectProperty ;
    rdfs:label "metricalContext"@en ;
    rdfs:comment "Categorisation depending on whether all lines have the same number of syllables"@en ;
    rdfs:domain default3:WorkPattern ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/metrical-context> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:nextFoot a owl:ObjectProperty ;
    rdfs:label "nextFoot"@en ;
    ns1:creator "mluisadiez" ;
    ns1:date "2018-06-16T20:30:43+00:00"^^xsd:dateTime ;
    rdfs:comment "This property points to the next instance of Foot"@en ;
    rdfs:domain default3:Foot ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range default3:Foot ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:equivalentProperty [ owl:inverseOf default3:previousFoot ] ;
    owl:inverseOf default3:previousFoot .

default3:nextFootAfterCaesura a owl:InverseFunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "nextFootAfterCaesura"@en ;
    ns1:creator "mluisadiez" ;
    ns1:date "2018-06-16T20:30:43+00:00"^^xsd:dateTime ;
    rdfs:comment "This property points to the next instance of Foot when this is positioned after a caesura"@en ;
    rdfs:domain default3:Foot ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range default3:Foot ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:equivalentProperty [ owl:inverseOf default3:previousFootBeforeCaesura ] ;
    owl:inverseOf default3:previousFootBeforeCaesura .

default3:nextUnit a owl:ObjectProperty ;
    rdfs:label "nextUnit"@en ;
    rdfs:comment "This property points to the next unit that forms the foot (an instance of FootDivision)"@en ;
    rdfs:domain default3:FootDivision ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range default3:FootDivision ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:equivalentProperty [ owl:inverseOf default3:previousUnit ] ;
    owl:inverseOf default3:previousUnit .

default3:patterningMetricalScheme a owl:DatatypeProperty ;
    rdfs:label "patterningMetricalScheme"@en ;
    ns1:creator "mluisadiez" ;
    ns1:date "2018-06-16T18:39:21+00:00"^^xsd:dateTime ;
    rdfs:comment "Metrical scheme for quantitative, tonal, accentual or accentual-syllabic metre" ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

default3:rhymeDispositionType a owl:ObjectProperty ;
    rdfs:label "rhymeDispositionType"@en ;
    rdfs:comment "Categorisation of the stanza according to the diposition of the rhymes (eg. “cross”, “pair”)"@en ;
    rdfs:domain default3:StanzaPattern ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/rhymeDisposition-type> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:rhymesWith a owl:ObjectProperty ;
    rdfs:label "rhymesWith"@en ;
    rdfs:comment "This property points to an instance of Line that rhymes with the current Line under analysis"@en ;
    rdfs:domain default4:Line ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range default4:Line ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:successIn a owl:ObjectProperty ;
    rdfs:label "successIn"@en ;
    rdfs:comment "A metaplasm is presented in Line"@en ;
    rdfs:domain default3:Metaplasm ;
    rdfs:range default4:Line ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:typeOfMetaplasm a owl:ObjectProperty ;
    rdfs:label "typeOfMetaplasm"@en ;
    rdfs:comment "Type of Metaplasm"@en ;
    rdfs:domain default3:Metaplasm ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/metaplasm-type> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:typeOfRhymeMatching a owl:ObjectProperty ;
    rdfs:label "typeOfRhymeMatching"@en ;
    rdfs:comment "Explain the degree in which the different rhyming sounds match (e.g. assonant, consonant, perfect, imperfect, half-rhyme...)"@en ;
    rdfs:domain default3:RhymeMatch ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/rhymeMatching-type> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:unitType a owl:ObjectProperty ;
    rdfs:label "unitType"@en ;
    rdfs:comment "The type of unit that defines the foot division"@en ;
    rdfs:domain default3:FootDivision ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/footUnit-type> ;
            owl:onProperty ns4:inScheme ] ;
    rdfs:subPropertyOf owl:topObjectProperty .

default3:versificationType a owl:ObjectProperty ;
    rdfs:label "versificationType"@en ;
    rdfs:comment "Type of metre: accentual verse, accentual-syllabic verse, syllabic verse, quantitative verse, etc."@en ;
    rdfs:domain default3:WorkPattern ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-prosodicElements"@en ;
    rdfs:range [ a owl:Restriction ;
            owl:hasValue <http://postdata.linhd.uned.es/kos/versification-type> ;
            owl:onProperty ns4:inScheme ],
        ns4:Concept ;
    rdfs:subPropertyOf owl:topObjectProperty .

ns2:abstract a owl:AnnotationProperty,
        owl:DatatypeProperty ;
    rdfs:label "abstract" ;
    rdfs:comment "A summary of the resource."@en ;
    rdfs:subPropertyOf owl:topDataProperty .

