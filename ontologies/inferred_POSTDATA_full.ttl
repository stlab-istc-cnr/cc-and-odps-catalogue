    rdfs:comment "Comment or explanations"@en ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core"@en ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

:originalLanguage a owl:DatatypeProperty ;
    rdfs:label "original Language"@en ;
    rdfs:comment "The language from which the Redaction is a translation"@en,
        "The language from which the Redaction/Poetic Work is a translation"@en ;
    rdfs:domain :Redaction ;
    rdfs:isDefinedBy "http://postdata.linhd.uned.es/ontology/postdata-core/" ;
    rdfs:range [ a rdfs:Datatype ;
            owl:unionOf ( xsd:language xsd:string ) ],
