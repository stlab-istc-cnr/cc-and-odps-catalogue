>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isVariantTranslationOf">
    <rdfs:seeAlso xml:lang="en">isVariant</rdfs:seeAlso>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:comment xml:lang="en">A LinguisticObject is a slightly modified copy of another LinguisticObject.

For comparing between different languages (where a translation act is known to have taken place directly between those documents, in a particular direction)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isVersionInAnotherLanguageOf"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:label xml:lang="en">is variant translation of</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nb616c02fca6f41e3b4f27cef82173f66">
    <rdf:first rdf:resource="http://purl.org/saws/ontology#hasAddedMarginalia"/>
    <rdf:rest rdf:nodeID="Na5a8be557dde4c92970e5c52fa10a143"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isInSameFamilyAs">
    <rdfs:comment xml:lang="en">Two OriginalMaterials are related by being part of the same Family of Original Materials</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:range rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
    <rdfs:label xml:lang="en">is in same family as</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#decorationAddedBy">
    <rdfs:domain rdf:resource="http://purl.org/saws/ontology#DecorativeItem"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdfs:seeAlso xml:lang="en">Marginalia marginaliaAddedBy Scribe</rdfs:seeAlso>
    <rdfs:label xml:lang="en">decoration added by</rdfs:label>
    <rdfs:comment xml:lang="en">A DecorativeItem has been physically added onto the manuscript by a Person

NB to represent marginalia notes (text) physically written in the margins of the manuscript, use: Marginalia marginaliaAddedBy Scribe</rdfs:comment>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E21_Person"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#hasAddedDecoration"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isLongerVersionOf">
    <rdfs:seeAlso xml:lang="en">isLongerTranslationOf</rdfs:seeAlso>
    <rdfs:label xml:lang="en">is longer version of</rdfs:label>
    <rdfs:comment xml:lang="en">A LinguisticObject is related to another LinguisticObject in that it conveys the same ideas (FRBRoo: Work), but is expressed in an expanded or enhanced version of the text (FRBRoo: a different and longer Expression). This need not imply that one is a direct copy of the other, nor which is the earlier.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isVersionOf"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N3d4d0d4d8cdb4a82a909dc1d269189ef">
    <rdf:first rdf:resource="http://purl.org/saws/ontology#ScribeSection"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#Definition">
    <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#ContentItem"/>
    <rdfs:label xml:lang="en">definition</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en">A content item that is defining a concept or term used in the material</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N6e92033ab8784a75a4bd38341ddb62f9">
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    <rdf:first rdf:resource="http://purl.org/saws/ontology#ReconstructedManuscript"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isOlderThan">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">A Physical man-made thing (physical manuscript) is older than (i.e. was produced earlier in time than) another specified Physical man-made thing</rdfs:comment>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E18_Physical_Thing"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#isYoungerThan"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E18_Physical_Thing"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdfs:label xml:lang="en">is older than</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isCloseTranslationOf">
    <rdfs:comment xml:lang="en">This property indicates that the translated text is a close interpretation of the original text. This need not imply that one is a direct translation of the other, nor which is the earlier</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isVersionInAnotherLanguageOf"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:seeAlso xml:lang="en">isLooseTranslationOf
isCloseRenderingOf</rdfs:seeAlso>
    <rdfs:label xml:lang="en">is close translation of</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isLocationForWritingOf">
    <rdfs:domain rdf:nodeID="N0a53574c9a9c4748a278a29ca3b61eab"/>
    <rdfs:seeAlso xml:lang="en">isLocationOf</rdfs:seeAlso>
    <rdfs:label xml:lang="en">is location for writing of</rdfs:label>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:comment xml:lang="en">A Place is the location witnessing the writing (or transcription) of a LinguisticObject</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#isWrittenAt"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isIdentifiedBy">
    <rdfs:comment xml:lang="en">A ScribeSection of text within the source isIdentifiedBy a Scribe/An EditorSection of text within the source isIdentifiedBy a modern-day Editor</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P94i_was_created_by"/>
    <rdfs:range rdf:nodeID="N8e82d82ba44448aa808c1db00b13c2ca"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is identified by</rdfs:label>
    <rdfs:domain rdf:nodeID="N5e2219dd8b4944e491ed0151ee0ff31d"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isTwinOf">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isSiblingOf"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:comment xml:lang="en">The two OriginalMaterials are produced using the same direct source OriginalMaterial(s)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is twin of</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#Institution">
    <rdfs:comment xml:lang="en">An Institution can own materials, be located at places, and include persons as members</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">institution</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E74_Group"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isShorterTranslationOf">
    <rdfs:seeAlso xml:lang="en">isShorterVersionOf</rdfs:seeAlso>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isVersionInAnotherLanguageOf"/>
    <rdfs:comment xml:lang="en">A Linguistic Object is a condensed version of another Linguistic Object, in a different language. This need not imply that one is a direct translation of the other, nor which is the earlier]</rdfs:comment>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">is shorter translation of</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isRelatedTo">
    <rdfs:label xml:lang="en">is related to</rdfs:label>
    <rdfs:comment xml:lang="en">Two things are related in some way. This relation may be used where the details of the link are to be refined at a later date, or where some relation is seen but the annotater cannot be more specific than to indicate that this relationship exists.</rdfs:comment>
    <owl:propertyDisjointWith rdf:resource="http://purl.org/saws/ontology#isUnrelatedTo"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isTransmittedBy">
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#isTransmitterOf"/>
    <rdfs:range rdf:resource="http://purl.org/saws/ontology#Transmitter"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isWrittenBy"/>
    <rdfs:domain rdf:nodeID="Nbb564e31a55a4084bbc12cb321d9c7e7"/>
    <rdfs:label xml:lang="en">is transmitted by</rdfs:label>
    <rdfs:comment xml:lang="en">A LinguisticObject (except Edition) is transmitted by this Transmitter (Person)</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#areMarginaliaOn">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">To indicate that Marginalia have been written on the manuscript,e .g. in the margins</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
    <rdfs:domain rdf:resource="http://purl.org/saws/ontology#Marginalia"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#hasMarginalia"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#refersTo"/>
    <rdfs:label xml:lang="en">are marginalia on</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N12f7998d0d8941109aece7b28414f6b0">
    <owl:unionOf rdf:nodeID="N5a50c457a0734bfbab5000533835ecb7"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#physicalLocationOf">
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#physicallyLocatedAt"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:label xml:lang="en">physical location of</rdfs:label>
    <rdfs:domain rdf:resource="http://purl.org/saws/ontology#Locus"/>
    <rdfs:comment xml:lang="en">This lets you describe the physical location of the LinguisticObject, e.g. a particular piece of text, or Marginalia, by recording the Locus that the notes are physically written on in a Material. You can describe the Locus (e.g. folio number, or position on the page) in free text, in as much detail as required, if there is no formal way to describe the location of the text/marginalia] 

NB Use this relation if you are talking about the physical location for an abstract linguistic object. Use isLocationOf if you are talking about the physical location of a physically-existing object.</rdfs:comment>
    <rdfs:seeAlso xml:lang="en">isLocationOf</rdfs:seeAlso>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#DecorativeItem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E36_Visual_Item"/>
    <rdfs:label xml:lang="en">decorative item</rdfs:label>
    <rdfs:comment xml:lang="en">Decorative element (picture, ornamentation, etc) within the material, either meaning-bearing or non meaning-bearing</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nafd3c684b3a04adaa7fd7a8e75f4d895">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:intersectionOf rdf:nodeID="N497de0a39161425aab5fbdada2cbe84c"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#OriginalMaterial">
    <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#Material"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="en">original material</rdfs:label>
    <rdfs:comment xml:lang="en">A material produced by scribe transcription rather than being edited in modern times</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#contains">
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P10i_contains"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#fallsWithin"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#hasAncestor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label xml:lang="en">has ancestor</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:comment xml:lang="en">An ancestor OriginalMaterial is a source material (direct or indirect) for the production of part or all of the descendant OriginalMaterial</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isInSameFamilyAs"/>
    <owl:propertyDisjointWith rdf:resource="http://purl.org/saws/ontology#hasDescendant"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#isAncestorOf"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="Nb9cc46ca14cd4f4cadcc8a4a707d51ac">
    <rdf:first rdf:resource="http://purl.org/saws/ontology#CollectionInstance"/>
    <rdf:rest rdf:nodeID="Nca45c38fe18f4841aa48e0f394bf68d0"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N8c5997568fc34568b20c12e85e4dc0a0">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:unionOf rdf:nodeID="N67fc190ef62f4cdeb538da63dbec10b8"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isVersionOf">
    <rdfs:label xml:lang="en">is version of</rdfs:label>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P130_shows_features_of"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:comment xml:lang="en">A LinguisticObject is a version in the same language of another LinguisticObject. This relates to the other object, but matches less closely than isVariantOf. This need not imply that one is a direct copy of the other, nor which is the earlier.</rdfs:comment>
    <rdfs:seeAlso xml:lang="en">isVersionInAnotherLanguageOf</rdfs:seeAlso>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N329f8ba6e3ad4e25891b156f0a1f78d2">
    <rdf:rest rdf:nodeID="N54f2e28d73264291a70fe2ae24a99237"/>
    <rdf:first rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#ownedBy">
    <rdfs:range rdf:nodeID="N6bb4b9fcd6f84f17aa5980a3e1c36df4"/>
    <rdfs:comment xml:lang="en">The Material of interest has been owned (in the past or currently) by this particular Person. This would normally be based on a specific statement/mark of ownership.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://purl.org/saws/ontology#Material"/>
    <rdfs:label xml:lang="en">owned by</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#follows">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:label xml:lang="en">follows</rdfs:label>
    <rdfs:comment xml:lang="en">A Section/ContentItem/CollectionInstance comes after another Section/ContentItem/CollectionInstance in the document</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://purl.org/saws/ontology#precedes"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#isMemberOf">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E1_CRM_Entity"/>
    <rdfs:seeAlso xml:lang="en">dct:isPartOf
efrbroo:R10i_is_member_of
ecrm:P107i_is_current_or_former_member_of</rdfs:seeAlso>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    <rdfs:label xml:lang="en">is member of</rdfs:label>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E1_CRM_Entity"/>
    <rdfs:comment xml:lang="en">We use isMemberOf to indicate:
- An OriginalMaterial is member of a Family of related
OriginalMaterials
- A Person is member of an Institution</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N2a80907eabfe462ea1b27734f83df2ff">
    <rdf:rest rdf:nodeID="Nd9108ed279d5490198c8fc1dc1f21dad"/>
    <rdf:first rdf:resource="http://erlangen-crm.org/current/E53_Place"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#physicallyLocatedAt">
    <rdfs:comment xml:lang="en">physically located at</rdfs:comment>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/saws/ontology#isRelatedTo"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    <rdfs:seeAlso xml:lang="en">isLocatedAt</rdfs:seeAlso>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:comment xml:lang="en">This lets you describe the physical location of the LinguisticObject, e.g. a particular piece of text, or Marginalia, by recording the Locus that the notes are physically written on in a Material. You can describe the Locus (e.g. folio number, or position on the page) in free text, in as much detail as required, if there is no formal way to describe the location of the text/marginalia

NB Use this relation if you are talking about an abstract linguistic object which is to be found at a particular locus. Use isLocatedAt if you are talking about a physically-existing object being located at a physical place.</rdfs:comment>
    <rdfs:range rdf:resource="http://purl.org/saws/ontology#Locus"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/saws/ontology#Editor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Transmitter"/>
    <rdfs:label xml:lang="en">editor</rdfs:label>
    <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Scribe"/>
    <rdfs:comment xml:lang="en">A modern scholar editing material to produce an edition, in the age of printing and beyond</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E21_Person"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N5a50c457a0734bfbab5000533835ecb7">
    <rdf:first rdf:resource="http://purl.org/saws/ontology#ManuscriptPart"/>
