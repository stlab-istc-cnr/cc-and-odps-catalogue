###  http://culturalis.org/oad#contains
:contains rdf:type owl:ObjectProperty ;
          owl:equivalentProperty [ owl:inverseOf :isContainedIn
                                 ] ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :FindingAid ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( <http://culturalis.org/eac-cpf#Instance>
                                     :Instance
                                   )
                     ] ;
          rdfs:comment "Inverse property of oad:isContainedIn"@en ,
                       "Proprietà inversa di 'isContainedIn'."@it ;
          rdfs:label "Contains"@en ,
                     "Contiene"@it ;
          owl:versionInfo "Questa proprietà è stata aggiunta nella versione del 2 agosto 2018" .


###  http://culturalis.org/oad#describes
:describes rdf:type owl:ObjectProperty ;
           owl:equivalentProperty [ owl:inverseOf :isDescribedBy
                                  ] ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           owl:inverseOf :isDescribedBy ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :FindingAid ;
           rdfs:range :ArchivalResource ;
           rdfs:comment "La proprietà collega uno strumento di ricerca alla risorsa archivistica descritta"@it ,
                        "This functional property links a finding aid to the archival resource described"@en ;
           rdfs:label "Describes"@en ,
                      "Descrive"@it ;
           owl:versionInfo "Questa proprietà è stata aggiunta nella versione del 2 agosto 2018" .


###  http://culturalis.org/oad#hasAccessPoint
:hasAccessPoint rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
