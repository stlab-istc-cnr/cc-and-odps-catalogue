###  https://w3id.org/italia/onto/AtlasOfPaths/bestWhen
<https://w3id.org/italia/onto/AtlasOfPaths/bestWhen> rdf:type owl:ObjectProperty ;
                                                     rdfs:subPropertyOf <https://w3id.org/italia/onto/TI/hasTemporalCoverage> ;
                                                     rdfs:domain <https://w3id.org/italia/onto/AtlasOfPaths/PathPlan> ;
                                                     rdfs:range <https://w3id.org/italia/onto/TI/TemporalEntity> ;
                                                     rdfs:comment "Questa object property collega un piano di viaggio con un'entità temporale che specifica quale sia il periodo migliore dell'anno per percorrere un certo percorso."@it ,
                                                                  "This object property correlates a path plan with a temporal entity that specifies the best period of the year to walk a certain pathway."@en ;
                                                     rdfs:label "best when"@en ,
                                                                "periodo migliore"@it ;
                                                     owl:versionInfo "provvisoria"@it ,
                                                                     "unstable"@en .


###  https://w3id.org/italia/onto/AtlasOfPaths/encountersPath
<https://w3id.org/italia/onto/AtlasOfPaths/encountersPath> rdf:type owl:ObjectProperty ;
                                                           owl:equivalentProperty [ owl:inverseOf <https://w3id.org/italia/onto/AtlasOfPaths/encountersPath>
                                                                                  ] ;
                                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                                           owl:inverseOf <https://w3id.org/italia/onto/AtlasOfPaths/encountersPath> ;
                                                           rdf:type owl:SymmetricProperty ;
                                                           rdfs:domain <https://w3id.org/italia/onto/AtlasOfPaths/Path> ;
                                                           rdfs:range <https://w3id.org/italia/onto/AtlasOfPaths/Path> ;
                                                           owl:propertyChainAxiom ( <https://w3id.org/italia/onto/Route/hasRoute>
                                                                                    <https://w3id.org/italia/onto/l0/hasMember>
                                                                                    <https://w3id.org/italia/onto/AtlasOfPaths/encountersPathStage>
                                                                                    <https://w3id.org/italia/onto/l0/isMemberOf>
                                                                                    [ owl:inverseOf <https://w3id.org/italia/onto/Route/hasRoute>
                                                                                    ]
                                                                                  ) ;
                                                           rdfs:comment "Questa object property permette di collegare un cammino con gli altri cammini incontrati."@it ,
                                                                        "This object property specifies which paths encounter a considered path, which is the subject of triples having as predicate encountersPath."@en ;
                                                           rdfs:label "encounters path"@en ,
                                                                      "incontra cammino"@it ;
                                                           owl:versionInfo "stabile"@it ,
                                                                           "stable"@en .


