default3: a owl:Ontology ;
    rdfs:label "Art and Rare Materials Core Ontology"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    ns2:title "Art and Rare Materials Core Ontology"@en ;
    ns4:preferredNamespacePrefix "arm" ;
    ns3:extends <http://id.loc.gov/ontologies/bibframe/> ;
    ns3:specializes <http://id.loc.gov/ontologies/bibframe/> ;
    owl:versionIRI <https://w3id.org/arm/core/ontology/0.1/> ;
    owl:versionInfo "Version 0.1.0" ;
    ns1:definition "The Art and Rare Materials Core Ontology provides specialized modeling of bibliographic metadata in the art and rare materials domains. It is an extension to the BIBFRAME ontology for general bibliographic description."@en .

<https://w3id.org/arm/core/ontology/0.1/Arrangement> a owl:Class ;
    rdfs:label "Arrangement"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "Information about the organization and arrangement of a collection of items or a single item."@en ;
    ns1:example "For a scroll, the arrangement may be rolled or unrolled."@en,
        "For an individual book on display in an exhibition, the arrangement may be the page to which the book is opened for display."@en,
        "For computer files, organization and arrangement information may be the file structure and sort sequence of a file."@en,
        "For visual materials, this information may be how a collection is arranged."@en ;
    ns1:scopeNote "An Arrangement may be a NamedIndividual, a term from a controlled vocabulary, or an unnamed resource with an rdf:value."@en .

<https://w3id.org/arm/core/ontology/0.1/Autograph> a owl:Class ;
    rdfs:label "Autograph"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "A person's name written on a resource in his or her own hand as part of the creation or production process."@en ;
    ns1:editorialNote "As currently defined, excludes post-production autographs on an item, such as on a gift. This distinction is subject to future review."@en ;
    ns1:example "A binder's signature"@en .

<https://w3id.org/arm/core/ontology/0.1/BindersTicket> a owl:Class ;
    rdfs:label "Binder's ticket"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "Small engraved or printed label, usually found on the upper outside corner of one of the front flyleaves, giving the name and usually the address of the bookbinder; common in the 18th and early 19th centuries."@en .

<https://w3id.org/arm/core/ontology/0.1/Binding> a owl:Class ;
    rdfs:label "Binding"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Enclosure> ;
    ns1:definition "Covering of a book which holds the sheets together to provide protection, and collectively all the parts of a book applied in the process of attaching the leaves and covering them."@en .

<https://w3id.org/arm/core/ontology/0.1/BoundCollection> a owl:Class ;
    rdfs:label "Bound collection"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Item> ;
    ns1:definition "An artificial gathering of two or more items physically into or on a single carrier, whether bound, affixed, or otherwise brought together into a single bound unit, and lacking a collective title. Includes only collections gathered subsequent to publication, rather than issued or reissued together by the publisher."@en .

<https://w3id.org/arm/core/ontology/0.1/CaptionNote> a owl:Class ;
    rdfs:label "Caption note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:comment "Use separate notes for multiple captions."@en ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A distinctive caption independent of the resource title, including mottos and headings."@en .

<https://w3id.org/arm/core/ontology/0.1/Citation> a owl:Class ;
    rdfs:label "Citation"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A single citation within a bibliographic source."@en .

<https://w3id.org/arm/core/ontology/0.1/ConditionAssessment> a owl:Class ;
    rdfs:label "Condition assessment"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "Assessment and/or documentation of the physical condition of an item, including damage, material vulnerabilities, special storage requirements, etc. The assessment is generally the output of a condition assessment activity, and may or may not result in a conservator activity."@en .

<https://w3id.org/arm/core/ontology/0.1/ContentsNote> a owl:Class ;
    rdfs:label "Contents note"@en ;
    ns2:issued "2018-01-29T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note to provide a main contents of the resource rather than linking to an external table of contents or creating separate part/whole relationships to the entities that comprise the whole. Ideally, part whole relationships would be created instead of the this type of note."@en ;
    ns1:editorialNote "This note type was created based on http://bibliotek-o.org/1.1/ontology/specifyingContents. Likely belongs in a general bf:Notes extension, but need to add it here for now." .

<https://w3id.org/arm/core/ontology/0.1/DescriptiveNote> a owl:Class ;
    rdfs:label "Descriptive note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note that describes the resource."@en .

<https://w3id.org/arm/core/ontology/0.1/DustJacket> a owl:Class ;
    rdfs:label "DustJacket"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Enclosure> ;
    ns1:definition "A detachable, flexible covering for a book, usually of paper cut flush with the covers, folding around the binding with the ends tucked in between the cover boards and the free end paper."@en .

<https://w3id.org/arm/core/ontology/0.1/Entry> a owl:Class ;
    rdfs:label "Entry"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A specific entry within a bibliographic resource."@en ;
    ns1:example "An entry locating a citation within a bibliographic source."@en,
        "An exhibition catalog entry describing a specific item in the exhibition."@en ;
    ns1:scopeNote "Used in parsing out hierarchically structured location designators for citations."@en .

<https://w3id.org/arm/core/ontology/0.1/FontStyle> a owl:Class ;
    rdfs:label "Font style"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "Style of the font, e.g. bold, italic"@en .

<https://w3id.org/arm/core/ontology/0.1/Frame> a owl:Class ;
    rdfs:label "Frame"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Mount> ;
    ns1:definition "A supportive structure that surrounds or encloses an item."@en .

<https://w3id.org/arm/core/ontology/0.1/Handwriting> a owl:Class ;
    rdfs:label "Handwriting"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A specific example of a handwriting found in a textual resource. A handwriting can have a particular HandWritingType."@en .

<https://w3id.org/arm/core/ontology/0.1/InaccuracyNote> a owl:Class ;
    rdfs:label "Inaccuracy note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note explaining an unintentional or intentional inaccuracy, misspelling, or misprint relating to a specific resource."@en .

<https://w3id.org/arm/core/ontology/0.1/Inscription> a owl:Class ;
    rdfs:label "Inscription"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "Words, texts, lettering, or symbols written in manuscript on a resource."@en .

<https://w3id.org/arm/core/ontology/0.1/Label> a owl:Class ;
    rdfs:label "Label"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "A piece of paper, leather, fabric, or other material inscribed, printed, or engraved with information and affixed to a resource or part of a resource for identification or description."@en .

<https://w3id.org/arm/core/ontology/0.1/LimitationNote> a owl:Class ;
    rdfs:label "Limitation note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note relating to the size and nature of a print run limited to a specific quantity of copies produced, or a statement relating to the enumeration and nature of a copy in relation to the larger print run."@en .

<https://w3id.org/arm/core/ontology/0.1/OclcNumber> a owl:Class ;
    rdfs:label "OCLC number"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Identifier> ;
    ns1:definition "Alphanumeric code assigned by OCLC to a resource. "@en ;
    ns1:editorialNote "Deprecate this class if defined in BIBFRAME."@en .

<https://w3id.org/arm/core/ontology/0.1/Page> a owl:Class ;
    rdfs:label "Page"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:comment "Used in any page-level description, as well as in parsing out hierarchically structured location designators for citations."@en ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A single page within a resource."@en .

<https://w3id.org/arm/core/ontology/0.1/PaginationFoliation> a owl:Class ;
    rdfs:label "Pagination foliation"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Extent> ;
    ns1:definition "Sequential numbering of individual leaves, plates, pages (numbering on both sides), or columns (used as a sequence of numbering) of a resource."@en .

<https://w3id.org/arm/core/ontology/0.1/Pedestal> a owl:Class ;
    rdfs:label "Pedestal"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Mount> ;
    ns1:definition "A solid, fixed base or support on which an item or part of an item is mounted."@en .

<https://w3id.org/arm/core/ontology/0.1/PreferredCitation> a owl:Class ;
    rdfs:label "Preferred citation"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note providing a specific citation format for citing the resource."@en .

<https://w3id.org/arm/core/ontology/0.1/RelatedMaterialNote> a owl:Class ;
    rdfs:label "Related material note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note providing information on materials (e.g. archival materials or publications) related to the described resource."@en ;
    ns1:scopeNote "Use if information describing the related material cannot be parsed into the appropriate relationship."@en .

<https://w3id.org/arm/core/ontology/0.1/Seal> a owl:Class ;
    rdfs:label "Seal"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "An impression that has been made in wax, clay, or another soft material by the impact of a hard engraved surface with monograms, writing, numbers, symbols, or design in intaglio, generally used to authenticate documents or for decoration."@en .

<https://w3id.org/arm/core/ontology/0.1/SlipCase> a owl:Class ;
    rdfs:label "SlipCase"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Enclosure> ;
    ns1:definition "Protective container designed to protect a book or item with open end along one edge."@en .

<https://w3id.org/arm/core/ontology/0.1/Stamp> a owl:Class ;
    rdfs:label "Stamp"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "A mark that is impressed, printed, perforated, or embossed, made by a hard object, especially a die, block, or other tool, on a softer material such as paper or wax."@en .

<https://w3id.org/arm/core/ontology/0.1/StylePeriod> a owl:Class ;
    rdfs:label "Style/Period"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A defined style, historical or artistic period, movement, group, or school whose characteristics are represented in a work."@en .

<https://w3id.org/arm/core/ontology/0.1/SubjectNote> a owl:Class ;
    rdfs:label "Subject note"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "Textual information describing the subject or iconographic details of a resource."@en ;
    ns1:scopeNote "Use for textual notes only, not for subject terms from a controlled value vocabulary."@en .

<https://w3id.org/arm/core/ontology/0.1/SummaryNote> a owl:Class ;
    rdfs:label "Summary note"@en ;
    ns2:issued "2018-01-29T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note providing a summary of the resource."@en ;
    ns1:editorialNote "This note type was created based on http://bibliotek-o.org/1.1/ontology/summarizing. Likely belongs in a general bf:Notes extension, but need to add it here for now." .

<https://w3id.org/arm/core/ontology/0.1/SupplementaryContentNote> a owl:Class ;
    rdfs:label "Supplementary content note"@en ;
    ns2:issued "2018-01-29T00:00:00-04:00" ;
    rdfs:subClassOf <http://id.loc.gov/ontologies/bibframe/Note> ;
    ns1:definition "A note indicating the existence of supplementary content such as bibliographies, discographies, filmographies, and/or other bibliographic references in the described resource or in accompanying material."@en ;
    ns1:editorialNote "This note type was created based on http://bibliotek-o.org/1.1/ontology/describingSupplementaryContent. Likely belongs in a general bf:Notes extension, but need to add it here for now." .

<https://w3id.org/arm/core/ontology/0.1/Volume> a owl:Class ;
    rdfs:label ""@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:comment "Used in any volume-level description, as well as in parsing out hierarchically structured location designators for citations."@en ;
    rdfs:subClassOf owl:Thing ;
    ns1:definition "A single bibliographic or physical volume of a resource."@en .

<https://w3id.org/arm/core/ontology/0.1/Watermark> a owl:Class ;
    rdfs:label "Watermark"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Marking> ;
    ns1:definition "A faint design or device incorporated into paper as it is made and appearing as translucent or shaded areas visual only through transmitted light. Often used to give the name of the manufacturer and the date of the paper's manufacture; a symbol, seal, or initials are also common. Often used to detect and prevent counterfeiting and forgery."@en .

<https://w3id.org/arm/core/ontology/0.1/WrapAroundBand> a owl:Class ;
    rdfs:label "Wrap around band"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Enclosure> ;
    ns1:definition "A printed paper the length of the dust jacket but smaller in height and wrapped around the book. It typically has reviews and is used for promotional purposes; may be added post-publication."@en .

<https://w3id.org/arm/core/ontology/0.1/Wrapper> a owl:Class ;
    rdfs:label "Wrapper"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subClassOf <https://w3id.org/arm/core/ontology/0.1/Enclosure> ;
    ns1:definition "A flexible printed or unprinted cover, usually made of paper, attached to a book or pamphlet; commonly used as an inexpensive temporary binding in the late 18th and early 19th century."@en .

<https://w3id.org/arm/core/ontology/0.1/carries> a owl:ObjectProperty ;
    rdfs:label "carries"@en ;
    ns2:issued "2018-04-27T00:00:00-04:00" ;
    ns2:modified "2018-04-27T00:00:00-04:00" ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:equivalentProperty [ owl:inverseOf <https://w3id.org/arm/core/ontology/0.1/isOnCarrier> ] ;
