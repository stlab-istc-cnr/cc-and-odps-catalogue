###  http://purl.org/vocab/frbr/core#relatedEndeavour
<http://purl.org/vocab/frbr/core#relatedEndeavour> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf owl:topObjectProperty .


###  http://purl.org/vocab/frbr/extended#complementsExpression
:complementsExpression rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :isRelatedToExpression ;
                       rdfs:range <http://purl.org/vocab/frbr/core#Expression> ;
                       rdfs:isDefinedBy : ;
                       rdfs:label "complements expression"@en .


###  http://purl.org/vocab/frbr/extended#complementsWork
:complementsWork rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isRelatedToWork ;
                 rdfs:range <http://purl.org/vocab/frbr/core#Work> ;
                 rdfs:comment """Referential: cadenza, libretto, choreography, ending for unfinished work, etc.
Autonomous: incidental music, musical setting for a text, pendant, etc."""@en ;
                 rdfs:isDefinedBy : ;
                 rdfs:label "complements work"@en .


###  http://purl.org/vocab/frbr/extended#hasDependentPartExpression
:hasDependentPartExpression rdf:type owl:ObjectProperty ;
                            owl:equivalentProperty [ owl:inverseOf :isDependentPartOfExpression
                                                   ] ;
                            rdfs:subPropertyOf :hasPartExpression ,
                                               [ owl:inverseOf :isPartOfExpression
                                               ] ;
                            owl:inverseOf :isDependentPartOfExpression ;
                            rdfs:domain <http://purl.org/vocab/frbr/core#Expression> ;
                            rdfs:range <http://purl.org/vocab/frbr/core#Expression> ;
                            rdfs:isDefinedBy : ;
                            rdfs:label "has dependent part expression"@en .


