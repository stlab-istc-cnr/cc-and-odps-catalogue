#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                   rdfs:comment """An entity primarily responsible for making the resource.

Examples of a Creator include a person, an organization, or a service. Typically, the name of a Creator should be used to indicate the entity."""@en ;
                                   rdfs:label "has creator"@en .


###  http://purl.org/dc/terms/format
<http://purl.org/dc/terms/format> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdfs:comment """The file format, physical medium, or dimensions of the resource.

Examples of dimensions include size and duration. Recommended best practice is to use a controlled vocabulary such as the list of Internet Media Types."""@en ;
                                  rdfs:label "has format"@en .


###  http://purl.org/dc/terms/language
<http://purl.org/dc/terms/language> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                    rdfs:comment """A language of the resource. Examples include written, spoken, sign, and computer languages.

Recommended best practice is to use a controlled vocabulary such as RFC 4646 [http://www.ietf.org/rfc/rfc4646.txt]."""@en ;
                                    rdfs:label "has language"@en .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:ObjectProperty ;
                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                   rdfs:comment "A legal document giving official permission to do something with the resource."@en ;
                                   rdfs:label "has license"@en .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf owl:topObjectProperty ;
                                     rdfs:comment """An entity responsible for making the resource available.

Examples of a Publisher include a person, an organization, or a service. Typically, the name of a Publisher should be used to indicate the entity."""@en ;
                                     rdfs:label "has publisher"@en .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdfs:comment """Information about rights held in and over the resource.

Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights."""@en ;
                                  rdfs:label "has rights"@en .


###  http://purl.org/spar/fabio/hasDiscipline
<http://purl.org/spar/fabio/hasDiscipline> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty [ owl:inverseOf <http://purl.org/spar/fabio/isDisciplineOf>
                                                                  ] ;
                                           rdfs:subPropertyOf owl:topObjectProperty ;
                                           owl:inverseOf <http://purl.org/spar/fabio/isDisciplineOf> ;
                                           rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://purl.org/spar/fabio/TermDictionary>
                                                                       <http://purl.org/vocab/frbr/core#Endeavour>
                                                                     )
                                                       ] ;
                                           rdfs:range <http://purl.org/spar/fabio/SubjectDiscipline> ;
                                           owl:propertyDisjointWith <http://purl.org/spar/fabio/isSchemeOf> ;
                                           rdfs:comment "The discipline to which a subject vocabulary belongs."@en ;
                                           rdfs:label "has discipline"@en .


###  http://purl.org/spar/fabio/hasManifestation
<http://purl.org/spar/fabio/hasManifestation> rdf:type owl:ObjectProperty ;
                                              owl:equivalentProperty [ owl:inverseOf <http://purl.org/spar/fabio/isManifestationOf>
                                                                     ] ;
                                              rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                 [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                 ] ;
                                              owl:inverseOf <http://purl.org/spar/fabio/isManifestationOf> ;
                                              rdfs:domain <http://purl.org/vocab/frbr/core#Work> ;
                                              rdfs:range <http://purl.org/vocab/frbr/core#Manifestation> ;
                                              owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#realization>
                                                                       <http://purl.org/vocab/frbr/core#embodiment>
                                                                     ) ;
                                              rdfs:comment "A property linking a particular work to its manifestations.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                              rdfs:label "has manifestation"@en .


###  http://purl.org/spar/fabio/hasPlaceOfPublication
<http://purl.org/spar/fabio/hasPlaceOfPublication> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf owl:topObjectProperty ;
                                                   rdfs:domain <http://purl.org/vocab/frbr/core#Endeavour> ;
                                                   rdfs:range <http://purl.org/vocab/frbr/core#Place> ;
                                                   rdfs:comment "The place (usually, the city) where the publisher of a particular bibliographic resource is located."@en ;
                                                   rdfs:label "has place of publication"@en .


###  http://purl.org/spar/fabio/hasPortrayal
<http://purl.org/spar/fabio/hasPortrayal> rdf:type owl:ObjectProperty ;
                                          owl:equivalentProperty [ owl:inverseOf <http://purl.org/spar/fabio/isPortrayalOf>
                                                                 ] ;
                                          rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                             [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                             ] ;
                                          owl:inverseOf <http://purl.org/spar/fabio/isPortrayalOf> ;
                                          rdfs:domain <http://purl.org/vocab/frbr/core#Work> ;
                                          rdfs:range <http://purl.org/vocab/frbr/core#Item> ;
                                          owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#realization>
                                                                   <http://purl.org/vocab/frbr/core#embodiment>
                                                                   <http://purl.org/vocab/frbr/core#exemplar>
                                                                 ) ;
                                          rdfs:comment "A property linking a particular work to its items.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                          rdfs:label "has portrayal"@en .


###  http://purl.org/spar/fabio/hasPrimarySubjectTerm
<http://purl.org/spar/fabio/hasPrimarySubjectTerm> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <http://purl.org/spar/fabio/hasSubjectTerm> ;
                                                   rdfs:comment "This property is used to associate a frbr:Endeavour to a term in a particular classification system - and the term is considered one of the main topics for the endeavour in consideration."@en ;
                                                   rdfs:label "has primary subject term"@en .


###  http://purl.org/spar/fabio/hasRepresentation
<http://purl.org/spar/fabio/hasRepresentation> rdf:type owl:ObjectProperty ;
                                               owl:equivalentProperty [ owl:inverseOf <http://purl.org/spar/fabio/isRepresentationOf>
                                                                      ] ;
                                               rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                  [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                  ] ;
                                               owl:inverseOf <http://purl.org/spar/fabio/isRepresentationOf> ;
                                               rdfs:domain <http://purl.org/vocab/frbr/core#Expression> ;
                                               rdfs:range <http://purl.org/vocab/frbr/core#Item> ;
                                               owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#embodiment>
                                                                        <http://purl.org/vocab/frbr/core#exemplar>
                                                                      ) ;
                                               rdfs:comment "A property linking a particular expression to its items.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                               rdfs:label "has representation"@en .


###  http://purl.org/spar/fabio/hasSubjectTerm
<http://purl.org/spar/fabio/hasSubjectTerm> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                            rdfs:domain <http://purl.org/vocab/frbr/core#Endeavour> ;
                                            rdfs:range <http://purl.org/spar/fabio/SubjectTerm> ;
                                            rdfs:comment "This property is used to associate a frbr:Endeavour to a term in a particular classification system."@en ;
                                            rdfs:label "has subject term"@en .


###  http://purl.org/spar/fabio/isDisciplineOf
<http://purl.org/spar/fabio/isDisciplineOf> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:topObjectProperty ;
                                            rdfs:domain <http://purl.org/spar/fabio/SubjectDiscipline> ;
                                            rdfs:range [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://purl.org/spar/fabio/TermDictionary>
                                                                       <http://purl.org/vocab/frbr/core#Endeavour>
                                                                     )
                                                       ] ;
                                            owl:propertyDisjointWith <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                            rdfs:comment "This property relates a subject vocabulary to the discipline to which it belongs."@en ;
                                            rdfs:label "is discipline of"@en .


###  http://purl.org/spar/fabio/isManifestationOf
<http://purl.org/spar/fabio/isManifestationOf> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                  [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                  ] ;
                                               rdfs:domain <http://purl.org/vocab/frbr/core#Manifestation> ;
                                               rdfs:range <http://purl.org/vocab/frbr/core#Work> ;
                                               owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#embodimentOf>
                                                                        <http://purl.org/vocab/frbr/core#realizationOf>
                                                                      ) ;
                                               rdfs:comment "A property linking a particular manifestation to the work it is manifesting.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                               rdfs:label "is manifestation of"@en .


###  http://purl.org/spar/fabio/isPortrayalOf
<http://purl.org/spar/fabio/isPortrayalOf> rdf:type owl:ObjectProperty ;
                                           rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                              [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                              ] ;
                                           rdfs:domain <http://purl.org/vocab/frbr/core#Item> ;
                                           rdfs:range <http://purl.org/vocab/frbr/core#Work> ;
                                           owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#exemplarOf>
                                                                    <http://purl.org/vocab/frbr/core#embodimentOf>
                                                                    <http://purl.org/vocab/frbr/core#realizationOf>
                                                                  ) ;
                                           rdfs:comment "A property linking a particular item to the work it portrays.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                           rdfs:label "is portrayal of"@en .


###  http://purl.org/spar/fabio/isRepresentationOf
<http://purl.org/spar/fabio/isRepresentationOf> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                   [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                   ] ;
                                                rdfs:domain <http://purl.org/vocab/frbr/core#Item> ;
                                                rdfs:range <http://purl.org/vocab/frbr/core#Expression> ;
                                                owl:propertyChainAxiom ( <http://purl.org/vocab/frbr/core#exemplarOf>
                                                                         <http://purl.org/vocab/frbr/core#embodimentOf>
                                                                       ) ;
                                                rdfs:comment "A property linking a particular item to the expression it represents.  This property is additional to the relationships between FRBR endeavours present in the classical FRBR data model."@en ;
                                                rdfs:label "is representation of"@en .


###  http://purl.org/spar/fabio/isSchemeOf
<http://purl.org/spar/fabio/isSchemeOf> rdf:type owl:ObjectProperty ;
                                        owl:equivalentProperty [ owl:inverseOf <http://www.w3.org/2004/02/skos/core#inScheme>
                                                               ] ;
                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                        owl:inverseOf <http://www.w3.org/2004/02/skos/core#inScheme> ;
                                        rdf:type owl:AsymmetricProperty ,
                                                 owl:IrreflexiveProperty ;
                                        rdfs:domain <http://www.w3.org/2004/02/skos/core#ConceptScheme> ;
                                        rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
                                        rdfs:comment "This property expresses the fact that a scheme contains a concept."@en ;
                                        rdfs:label "is scheme of"@en .


###  http://purl.org/spar/fabio/isStoredOn
<http://purl.org/spar/fabio/isStoredOn> rdf:type owl:ObjectProperty ;
                                        owl:equivalentProperty [ owl:inverseOf <http://purl.org/spar/fabio/stores>
                                                               ] ;
                                        rdfs:subPropertyOf owl:topObjectProperty ;
                                        owl:inverseOf <http://purl.org/spar/fabio/stores> ;
                                        rdfs:domain <http://purl.org/spar/fabio/Item> ;
                                        rdfs:range <http://purl.org/spar/fabio/StorageMedium> ;
                                        rdfs:comment "This property relates a fabio:Item to the medium upon which it is stored."@en ;
                                        rdfs:label "is stored on"@en .


###  http://purl.org/spar/fabio/stores
<http://purl.org/spar/fabio/stores> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                    rdfs:domain <http://purl.org/spar/fabio/StorageMedium> ;
                                    rdfs:range <http://purl.org/spar/fabio/Item> ;
                                    rdfs:comment "This property relates a storage medium to the fabio:Item stored upon it."@en ;
                                    rdfs:label "stores"@en .


###  http://purl.org/vocab/frbr/core#abridgement
<http://purl.org/vocab/frbr/core#abridgement> rdf:type owl:ObjectProperty ;
                                              owl:equivalentProperty [ owl:inverseOf <http://purl.org/vocab/frbr/core#abridgementOf>
                                                                     ] ;
                                              rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                 [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                 ] ;
                                              owl:inverseOf <http://purl.org/vocab/frbr/core#abridgementOf> ;
                                              rdfs:domain <http://purl.org/vocab/frbr/core#Expression> ;
                                              rdfs:range <http://purl.org/vocab/frbr/core#Expression> ;
                                              rdfs:comment """An expression abridged in another one.

In the abridged expression some content of the previous expression is removed, but the result does not alter the content to the extent that it becomes a new work. The expressions resulting from such modification are generally autonomous in nature (i.e., they do not normally require reference to the prior expression in order to be used or understood). """@en ;
                                              rdfs:label "has abridgement"@en .


###  http://purl.org/vocab/frbr/core#abridgementOf
<http://purl.org/vocab/frbr/core#abridgementOf> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                   [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                   ] ;
                                                rdfs:comment "It identifies the entire expression of an abridged one."@en ;
                                                rdfs:label "is abridgement of"@en .


###  http://purl.org/vocab/frbr/core#adaption
<http://purl.org/vocab/frbr/core#adaption> rdf:type owl:ObjectProperty ;
                                           owl:equivalentProperty [ owl:inverseOf <http://purl.org/vocab/frbr/core#adaptionOf>
                                                                  ] ;
                                           rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                              [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                              ] ;
                                           owl:inverseOf <http://purl.org/vocab/frbr/core#adaptionOf> ;
                                           rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://purl.org/vocab/frbr/core#Expression>
                                                                       <http://purl.org/vocab/frbr/core#Work>
                                                                     )
                                                       ] ;
                                           rdfs:range [ rdf:type owl:Class ;
                                                        owl:unionOf ( <http://purl.org/vocab/frbr/core#Expression>
                                                                      <http://purl.org/vocab/frbr/core#Work>
                                                                    )
                                                      ] ;
                                           rdfs:comment """A work/expression adapted in another one.

This property describe the modification of an original work that is sufficient in degree to warrant their being considered as new works, rather than simply different expressions of the same work. If there exists a relation of this kind among two different expressions, they always refer to different works."""@en ;
                                           rdfs:label "has adaption"@en .


###  http://purl.org/vocab/frbr/core#adaptionOf
<http://purl.org/vocab/frbr/core#adaptionOf> rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                                [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                                ] ;
                                             rdfs:comment "It identifies the work/expression of an adapted one."@en ;
                                             rdfs:label "is adaption of"@en .


###  http://purl.org/vocab/frbr/core#alternate
<http://purl.org/vocab/frbr/core#alternate> rdf:type owl:ObjectProperty ;
                                            owl:equivalentProperty [ owl:inverseOf <http://purl.org/vocab/frbr/core#alternateOf>
                                                                   ] ;
                                            rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#relatedEndeavour> ,
                                                               [ owl:inverseOf <http://purl.org/vocab/frbr/core#relatedEndeavour>
                                                               ] ;
                                            owl:inverseOf <http://purl.org/vocab/frbr/core#alternateOf> ;
                                            rdfs:domain <http://purl.org/vocab/frbr/core#Manifestation> ;
                                            rdfs:range <http://purl.org/vocab/frbr/core#Manifestation> ;
                                            rdfs:comment """A manifestation having another one as alternate.

The alternate relationship involves manifestations that effectively serve as alternates for each other. The alternate relationship obtains, for example, when a publication, sound recording, video, etc. is issued in more than one format or when it is released simultaneously by different publishers in different countries."""@en ;
                                            rdfs:label "has alternate"@en .


