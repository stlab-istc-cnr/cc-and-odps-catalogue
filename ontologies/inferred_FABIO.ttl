                                                          owl:someValuesFrom <http://purl.org/spar/fabio/AnalogStorageMedium>
                                                        ] ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://purl.org/vocab/frbr/core#exemplarOf> ;
                                                          owl:someValuesFrom <http://purl.org/spar/fabio/AnalogManifestation>
                                                        ] ;
                                        owl:disjointWith <http://purl.org/spar/fabio/DigitalItem> ;
                                        <http://purl.org/dc/elements/1.1/description> "An analog item is an exemplar of an analog manifestation only and it is always stored in a storage medium suitable for analog objects, such as paper, vinyl discs and films."@en ;
                                        rdfs:comment "A real object that is an exemplar of a fabio:Manifestation, such as a particular copy of the book 'Alice's adventures in Wonderland', that a person may own."@en ;
                                        rdfs:label "analog item"@en .


###  http://purl.org/spar/fabio/AnalogManifestation
<http://purl.org/spar/fabio/AnalogManifestation> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/Manifestation> ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://purl.org/vocab/frbr/core#exemplar> ;
                                                                   owl:someValuesFrom <http://purl.org/spar/fabio/AnalogItem>
                                                                 ] ;
                                                 owl:disjointWith <http://purl.org/spar/fabio/DigitalManifestation> ;
                                                 rdfs:comment "A manifestation in an analog form."@en ;
                                                 rdfs:label "analog manifestation"@en .


###  http://purl.org/spar/fabio/AnalogStorageMedium
<http://purl.org/spar/fabio/AnalogStorageMedium> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/StorageMedium> ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://purl.org/spar/fabio/stores> ;
                                                                   owl:allValuesFrom <http://purl.org/spar/fabio/AnalogItem>
                                                                 ] ;
                                                 rdfs:comment "A means of storing information in non-digital form, e.g. paper, film (for analogue photographs or movies), magnetic tape (for analogue sound recordings or video recordings) or vinyl disc."@en ;
                                                 rdfs:label "analog storage medium"@en .


###  http://purl.org/spar/fabio/Announcement
<http://purl.org/spar/fabio/Announcement> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/spar/fabio/Work> ;
                                          rdfs:comment "A formal statement about something."@en ;
                                          rdfs:label "announcement"@en .


###  http://purl.org/spar/fabio/Anthology
<http://purl.org/spar/fabio/Anthology> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://purl.org/spar/fabio/ExpressionCollection> ;
                                       rdfs:comment "A collection of selected literary or scholastics works, for example poems, short stories, plays or research papers."@en ;
                                       rdfs:label "anthology"@en .


###  http://purl.org/spar/fabio/ApplicationProfile
<http://purl.org/spar/fabio/ApplicationProfile> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://purl.org/spar/fabio/Metadata> ;
                                                rdfs:comment "A set of metadata elements, policies and guidelines defined for a particular application.  The metadata elements used in the application profile may be drawn from more than one element sets, including locally defined sets. "@en ;
                                                rdfs:label "application profile"@en .


###  http://purl.org/spar/fabio/ApplicationProgrammingInterface
<http://purl.org/spar/fabio/ApplicationProgrammingInterface> rdf:type owl:Class ;
                                                             rdfs:subClassOf <http://purl.org/spar/fabio/ComputerProgram> ;
                                                             rdfs:comment " A computer program that enables a separate computer to interact programmatically with the computer running the API.  (Commonly abbreviated 'API'.)"@en ;
                                                             rdfs:label "API"@en ,
                                                                        "application programming interface"@en .


###  http://purl.org/spar/fabio/ArchivalDocument
<http://purl.org/spar/fabio/ArchivalDocument> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                              rdfs:comment "An archival document is a realization of the content related to an archival record. It can be exemplified as a book, a document, a letter, a database, etc."@en ;
                                              rdfs:label "archival document"@en .


###  http://purl.org/spar/fabio/ArchivalDocumentSet
<http://purl.org/spar/fabio/ArchivalDocumentSet> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/ExpressionCollection> ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                                   owl:someValuesFrom <http://purl.org/spar/fabio/ArchivalDocument>
                                                                 ] ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                                   owl:allValuesFrom <http://purl.org/spar/fabio/ArchivalDocument>
                                                                 ] ;
                                                 rdfs:comment "A collection of archival document."@en ;
                                                 rdfs:label "archival document set"@en .


###  http://purl.org/spar/fabio/ArchivalRecord
<http://purl.org/spar/fabio/ArchivalRecord> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/spar/fabio/Work> ,
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://purl.org/vocab/frbr/core#realization> ;
                                                              owl:allValuesFrom <http://purl.org/spar/fabio/ArchivalDocument>
                                                            ] ;
                                            rdfs:comment "An archival record connotes a material created or received by a person, family, or organization, public or private, in the conduct of their affairs that is preserved because of the enduring value contained in the information it contains or as evidence of the function and the responsibilities of its creator."@en ;
                                            rdfs:label "archival record"@en .


###  http://purl.org/spar/fabio/ArchivalRecordSet
<http://purl.org/spar/fabio/ArchivalRecordSet> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://purl.org/spar/fabio/WorkCollection> ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                                 owl:someValuesFrom <http://purl.org/spar/fabio/ArchivalRecord>
                                                               ] ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                                 owl:allValuesFrom <http://purl.org/spar/fabio/ArchivalRecord>
                                                               ] ,
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty <http://purl.org/vocab/frbr/core#realization> ;
                                                                 owl:allValuesFrom <http://purl.org/spar/fabio/ArchivalDocumentSet>
                                                               ] ;
                                               rdfs:comment "A collection of archival records."@en ;
                                               rdfs:label "archival record set"@en .


###  http://purl.org/spar/fabio/Article
<http://purl.org/spar/fabio/Article> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                     rdfs:comment "The realization of a piece of writing on a particular topic, usually published within a periodical publication (e.g. journal, magazine and newspaper)."@en ;
                                     rdfs:label "article"@en .


###  http://purl.org/spar/fabio/ArtisticWork
<http://purl.org/spar/fabio/ArtisticWork> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/spar/fabio/Work> ;
                                          rdfs:comment "It describes any work regarded as art in its widest sense, including works from literature and music, visual art, etc."@en ;
                                          rdfs:label "artistic work"@en .


###  http://purl.org/spar/fabio/AudioDocument
<http://purl.org/spar/fabio/AudioDocument> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://purl.org/vocab/frbr/core#realizationOf> ;
                                                             owl:someValuesFrom <http://purl.org/spar/fabio/SoundRecording>
                                                           ] ;
                                           rdfs:comment "The realization of a sound recording."@en ;
                                           rdfs:label "audio document"@en .


###  http://purl.org/spar/fabio/AuthorityFile
<http://purl.org/spar/fabio/AuthorityFile> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/spar/fabio/ControlledVocabulary> ;
                                           rdfs:comment "A controlled vocabulary or official list that establishes, for consistency, the authoritative forms of headings, and the preferred terms or proper names to be used, when creating a catalogue or when indexing and searching a set of entities within a defined domain."@en ;
                                           rdfs:label "authority file"@en .


###  http://purl.org/spar/fabio/BachelorsThesis
<http://purl.org/spar/fabio/BachelorsThesis> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://purl.org/spar/fabio/Thesis> ;
                                             rdfs:comment "A thesis reporting a research project undertaken as part of an undergraduate course of education leading to a bachelor's degree."@en ;
                                             rdfs:label "bachelor's thesis"@en .


###  http://purl.org/spar/fabio/BibliographicDatabase
<http://purl.org/spar/fabio/BibliographicDatabase> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://purl.org/spar/fabio/Database> ;
                                                   rdfs:comment "A database providing an authoritative source of bibliographic information, for example PubMed (http://www.ncbi.nlm.nih.gov/pubmed), CrossRef Metadata Search (http://search.crossref.org/) and PubMed Central (http://www.ncbi.nlm.nih.gov/pmc/)."@en ;
                                                   rdfs:label "bibliographic database"@en .


###  http://purl.org/spar/fabio/BibliographicMetadata
<http://purl.org/spar/fabio/BibliographicMetadata> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://purl.org/spar/fabio/Metadata> ;
                                                   rdfs:comment "Standard bibliographic metadata describing an expression of a work.  To take the example of a journal article, bibliographic metadata typically include the authors' names, the date of publication, the title of the article, the journal name and volume number, the first and last page numbers, and the Digital Object Identifier (DOI)."@en ;
                                                   rdfs:label "bibliographic metadata"@en .


###  http://purl.org/spar/fabio/Biography
<http://purl.org/spar/fabio/Biography> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://purl.org/spar/fabio/Work> ;
                                       rdfs:comment "An account of the events, works and achievements, both personal and professional, of a person, either living or dead."@en ;
                                       rdfs:label "biography"@en .


###  http://purl.org/spar/fabio/Blog
<http://purl.org/spar/fabio/Blog> rdf:type owl:Class ;
                                  rdfs:subClassOf <http://purl.org/spar/fabio/WebManifestation> ;
                                  rdfs:comment "A Web publication medium containing blog posts."@en ;
                                  rdfs:label "blog"@en .


###  http://purl.org/spar/fabio/BlogPost
<http://purl.org/spar/fabio/BlogPost> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://purl.org/spar/fabio/Entry> ,
                                                      <http://purl.org/spar/fabio/WebContent> ;
                                      rdfs:comment "Information manifested in a blog, one of a set of periodic sequential entries containing commentary, descriptions of events, or other material such as images or videos, usually displayed in reverse-chronological order and usually maintained by an individual, or comments on such a post."@en ;
                                      rdfs:label "blog post"@en .


###  http://purl.org/spar/fabio/Book
<http://purl.org/spar/fabio/Book> rdf:type owl:Class ;
                                  rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                  rdfs:comment "A non-serial document that is complete in one volume or a designated finite number of volumes.  A book published by a publisher is usually  identified by an International Standard Book Number (ISBN), and may be manifested as a physical printed publication on paper bound in a hard or soft cover, or in electronic format as an 'e-book'."@en ;
                                  rdfs:label "book"@en .


###  http://purl.org/spar/fabio/BookChapter
<http://purl.org/spar/fabio/BookChapter> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://purl.org/spar/fabio/Chapter> ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty <http://purl.org/vocab/frbr/core#partOf> ;
                                                           owl:someValuesFrom <http://purl.org/spar/fabio/Book>
                                                         ] ;
                                         rdfs:comment "A defined chapter of a book, usually with a separate title or number."@en ;
                                         rdfs:label "book chapter"@en .


###  http://purl.org/spar/fabio/BookReview
<http://purl.org/spar/fabio/BookReview> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://purl.org/spar/fabio/Review> ;
                                        rdfs:comment "A written review and critical analysis of the content, scope and quality of a book or other monographic work."@en ;
                                        rdfs:label "book review"@en .


###  http://purl.org/spar/fabio/BookSeries
<http://purl.org/spar/fabio/BookSeries> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://purl.org/spar/fabio/Series> ,
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                          owl:someValuesFrom <http://purl.org/spar/fabio/Book>
                                                        ] ;
                                        owl:disjointWith <http://purl.org/spar/fabio/BookSet> ;
                                        rdfs:comment "A sequence of books having certain characteristics in common that are formally identified together as a group - for instance, the books in the Law, Governance and Technology Series published by Springer."@en ;
                                        rdfs:label "book series"@en .


###  http://purl.org/spar/fabio/BookSet
<http://purl.org/spar/fabio/BookSet> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://purl.org/spar/fabio/ExpressionCollection> ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                       owl:someValuesFrom <http://purl.org/spar/fabio/Book>
                                                     ] ;
                                     rdfs:comment "A set of books having certain characteristics in common that informally allow their identification together as a group - for instance, the books of the Harry Potter saga."@en ;
                                     rdfs:label "book set"@en .


###  http://purl.org/spar/fabio/BriefReport
<http://purl.org/spar/fabio/BriefReport> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ,
                                                         [ rdf:type owl:Class ;
                                                           owl:unionOf ( <http://purl.org/spar/fabio/RapidCommunication>
                                                                         <http://purl.org/spar/fabio/ReportDocument>
                                                                       )
                                                         ] ;
                                         rdfs:comment "A brief report document.  This term may also be used synonymously with Rapid Communication to mean 'A short rapidly published research article or conference paper, typically reporting significant research results that have been recently discovered, or a brief news item reporting such discoveries.'"@en ;
                                         rdfs:label "brief report"@en .


###  http://purl.org/spar/fabio/CallForApplications
<http://purl.org/spar/fabio/CallForApplications> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ,
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <http://purl.org/vocab/frbr/core#realizationOf> ;
                                                                   owl:someValuesFrom <http://purl.org/spar/fabio/Proposition>
                                                                 ] ;
                                                 rdfs:comment "A document published by a funding agency requesting submission of applications for financial grants to fund projects, for example to enable research investigations in areas specified in the Call."@en ;
                                                 rdfs:label "call for applications" .


###  http://purl.org/spar/fabio/CaseForSupport
<http://purl.org/spar/fabio/CaseForSupport> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/spar/fabio/Work> ;
                                            rdfs:comment "A part of a grant application that provides a description of a proposed project and gives reasons why it is worthy of funding. (See also fabio:GrantApplication)."@en ;
                                            rdfs:label "case for support"@en .


###  http://purl.org/spar/fabio/CaseForSupportDocument
<http://purl.org/spar/fabio/CaseForSupportDocument> rdf:type owl:Class ;
                                                    rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ,
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://purl.org/vocab/frbr/core#realizationOf> ;
                                                                      owl:someValuesFrom <http://purl.org/spar/fabio/CaseForSupport>
                                                                    ] ;
                                                    rdfs:comment "A document containing the case for support for a particular project, usually contained within a grant application document but sometimes distributed separately, without the financial and organizational information that the grant application document also contains."@en ;
                                                    rdfs:label "case for support document"@en .


###  http://purl.org/spar/fabio/CaseReport
<http://purl.org/spar/fabio/CaseReport> rdf:type owl:Class ;
                                        rdfs:subClassOf <http://purl.org/spar/fabio/Report> ;
                                        rdfs:comment "A report about a particular case or situation."@en ;
                                        rdfs:label "case report"@en .


###  http://purl.org/spar/fabio/Catalog
<http://purl.org/spar/fabio/Catalog> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://purl.org/spar/fabio/Specification> ;
                                     rdfs:comment "A list of items describing the content of a resource, for example items in an exhibition, items offered for sale by a vendor, or entities contained within a library or collection.  Ideally, catalogs are created according to specific and uniform principles of construction and are under the control of an authority file."@en ;
                                     rdfs:label "catalog"@en .


###  http://purl.org/spar/fabio/Chapter
<http://purl.org/spar/fabio/Chapter> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                     rdfs:comment "A defined document section, forming part of or intended for inclusion within a larger document, usually with its own title or chapter number.  Different chapters within a document such as a book or a report may each be independently authored, or may all be authored by a single individual or group of authors."@en ;
                                     rdfs:label "chapter"@en .


###  http://purl.org/spar/fabio/CitationMetadata
<http://purl.org/spar/fabio/CitationMetadata> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://purl.org/spar/fabio/Metadata> ;
                                              rdfs:comment "Metadata describing the citations made within a work to other works, and (optionally) some characteristics of the expressions of the cited works."@en ;
                                              rdfs:label "citation metadata"@en .


###  http://purl.org/spar/fabio/ClinicalCaseReport
<http://purl.org/spar/fabio/ClinicalCaseReport> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://purl.org/spar/fabio/CaseReport> ;
                                                rdfs:comment "A presentation of findings following a clinical or medical investigation on a human or animal patient, that may contain a diagnosis and proposals for therapeutic treatment and/or epidemiological control measures, or may propose further evaluative studies that will eventually lead to such outcomes."@en ;
                                                rdfs:label "clinical case report"@en .


###  http://purl.org/spar/fabio/ClinicalGuideline
<http://purl.org/spar/fabio/ClinicalGuideline> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://purl.org/spar/fabio/InstructionalWork> ;
                                               rdfs:comment "A recommendation on the appropriate treatment and care of people with a specific disease or condition, based on the best available evidence, designed to help healthcare professionals in their work."@en ;
                                               rdfs:label "clinical guideline"@en .


###  http://purl.org/spar/fabio/ClinicalTrialDesign
<http://purl.org/spar/fabio/ClinicalTrialDesign> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/Specification> ;
                                                 rdfs:comment "A predefined written procedural method, designed to ensure reliability of findings, for undertaking a medical or veterinary clinical study of the safety, efficacy, or optimum dosage schedule of one or more diagnostic, therapeutic or prophylactic drugs or treatments, or of devices or techniques, involving a randomized controlled trial for evidence-based assessment in humans or animals, specifying criteria of eligibility, nature of controls, sampling schedules, data collection parameters, statistical analyses, reporting standards, etc. to be employed in undertaking the clinical trial." ;
                                                 rdfs:label "clinical trial design"@en .


###  http://purl.org/spar/fabio/ClinicalTrialReport
<http://purl.org/spar/fabio/ClinicalTrialReport> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/TrialReport> ;
                                                 rdfs:comment "The report of a pre-planned medical or veterinary clinical study of the safety, efficacy, or optimum dosage schedule of one or more diagnostic, therapeutic or prophylactic drugs, or of devices, treatments or techniques, involving a randomized controlled trial for evidence-based assessment in humans or animals selected according to predetermined criteria of eligibility and observed for evidence of favourable and unfavourable effects."@en ;
                                                 rdfs:label "clinical trial report"@en .


###  http://purl.org/spar/fabio/CollectedWorks
<http://purl.org/spar/fabio/CollectedWorks> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/spar/fabio/Anthology> ;
                                            rdfs:comment "A collection of the literary or scholastic works of a single person."@en ;
                                            rdfs:label "collected works"@en .


###  http://purl.org/spar/fabio/Comment
<http://purl.org/spar/fabio/Comment> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                     rdfs:comment "A verbal or written remark concerning some entity.  In written form, a comment is often appended to that entity and termed an annotation.  Within computer programs or ontologies, comments are added to enhance human understanding, and are usually prefaced by a special syntactic symbol that ensures they are ignored during execution of the program."@en ;
                                     rdfs:label "comment"@en .


###  http://purl.org/spar/fabio/CompleteWorks
<http://purl.org/spar/fabio/CompleteWorks> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://purl.org/spar/fabio/CollectedWorks> ;
                                           rdfs:comment "A collection of all the literary or scholastic works of a single person."@en ;
                                           rdfs:label "complete works"@en .


###  http://purl.org/spar/fabio/ComputerApplication
<http://purl.org/spar/fabio/ComputerApplication> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/spar/fabio/ComputerProgram> ;
                                                 rdfs:comment "A computer program designed to assist a human user to perform one or more goal-oriented tasks such as word processing or image processing.  A computer application will typically save its output files in one or more specific formats, conforming either to proprietary or open standards.  "@en ;
                                                 rdfs:label "computer application"@en .


###  http://purl.org/spar/fabio/ComputerFile
<http://purl.org/spar/fabio/ComputerFile> rdf:type owl:Class ;
                                          rdfs:subClassOf <http://purl.org/spar/fabio/DigitalItem> ;
                                          rdfs:comment "A digital item containing information in computer-readable form encoded in a particular format."@en ;
                                          rdfs:label "computer file"@en .


###  http://purl.org/spar/fabio/ComputerProgram
<http://purl.org/spar/fabio/ComputerProgram> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                             rdfs:comment "A unit of computer code in source or compiled form, employing one or more algorithms to be executed by a digital computer to undertake a particular task.  Computer programs are collectively called 'software' to distinguish them from the equipment ('hardware') upon which they run. "@en ;
                                             rdfs:label "computer program"@en .


###  http://purl.org/spar/fabio/ConferencePaper
<http://purl.org/spar/fabio/ConferencePaper> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://purl.org/spar/fabio/ProceedingsPaper> ;
                                             rdfs:comment "A paper, typically the realization of a research paper reporting original research findings, usually published within a conference proceedings volume."@en ;
                                             rdfs:label "conference paper"@en .


###  http://purl.org/spar/fabio/ConferencePoster
<http://purl.org/spar/fabio/ConferencePoster> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://purl.org/spar/fabio/Expression> ;
                                              rdfs:comment "A display poster, typically containing text with illustrative figures and/or tables, usually reporting research results or proposing hypotheses, submitted for acceptance to and/or presented at a conference, seminar, symposium, workshop or similar event."@en ;
                                              rdfs:label "conference poster"@en .


###  http://purl.org/spar/fabio/ConferenceProceedings
<http://purl.org/spar/fabio/ConferenceProceedings> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://purl.org/spar/fabio/AcademicProceedings> ,
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty <http://purl.org/vocab/frbr/core#part> ;
                                                                     owl:someValuesFrom <http://purl.org/spar/fabio/ConferencePaper>
                                                                   ] ;
                                                   rdfs:comment "A document containing the programme and collected conference papers, or their abstracts, presented at a conference, seminar, symposium or similar event."@en ;
                                                   rdfs:label "conference proceedings"@en .


###  http://purl.org/spar/fabio/ControlledVocabulary
<http://purl.org/spar/fabio/ControlledVocabulary> rdf:type owl:Class ;
                                                  rdfs:subClassOf <http://purl.org/spar/fabio/Vocabulary> ;
