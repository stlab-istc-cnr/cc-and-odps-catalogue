###  http://www.europeana.eu/schemas/edm/aggregatedCHO
:aggregatedCHO rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/subject> ,
                                  <http://www.cidoc-crm.org/rdfs/cidoc-crm#P129_is_about> ,
                                  <http://www.openarchives.org/ore/terms/aggregates> ;
               rdf:type owl:InverseFunctionalProperty ;
               rdfs:domain <http://www.openarchives.org/ore/terms/Aggregation> ;
               rdfs:range :ProvidedCHO ;
               rdfs:label "Aggregated Cultural Heritage Object"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "This property associates an ORE aggregation with the cultural heritage object(s) (CHO for short) it is about."@en .


###  http://www.europeana.eu/schemas/edm/begin
:begin rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :isRelatedTo ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Agent
                                   :TimeSpan
                                 )
                   ] .


###  http://www.europeana.eu/schemas/edm/collectionName
:collectionName rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:label "Collection Name"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "This property holds the collection identifier given to the dataset in Europeana."@en ;
                <http://www.w3.org/2004/02/skos/core#note> "The value of this property is provided by Europeana as part of the ingestion process."@en .


###  http://www.europeana.eu/schemas/edm/currentLocation
:currentLocation rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :ProvidedCHO
                                             [ owl:intersectionOf ( <http://www.openarchives.org/ore/terms/Proxy>
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.openarchives.org/ore/terms/proxyFor> ;
                                                                      owl:someValuesFrom :ProvidedCHO
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ]
                                           )
                             ] ;
                 rdfs:range :Place .


###  http://www.europeana.eu/schemas/edm/dataProvider
:dataProvider rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain <http://www.openarchives.org/ore/terms/Aggregation> ;
              rdfs:range :Agent .


###  http://www.europeana.eu/schemas/edm/end
:end rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf :isRelatedTo ;
     rdfs:domain [ rdf:type owl:Class ;
                   owl:unionOf ( :Agent
                                 :TimeSpan
                               )
                 ] .


###  http://www.europeana.eu/schemas/edm/europeanaProxy
:europeanaProxy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:label "Europeana Proxy"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "This property serves only as a flag to indicate that a proxy is a Europeana proxy (as opposed to a provider proxy).  It is for internal use only."@en ;
                <http://www.w3.org/2004/02/skos/core#note> "By default, any proxy without this flag can be interpreted as having the value false and  is a provider proxy."@en .


###  http://www.europeana.eu/schemas/edm/happenedAt
:happenedAt rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :Event ;
            rdfs:range :Place ;
            rdfs:label "Happened At"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> """This property associates an event with the place at which the event
happened."""@en .


###  http://www.europeana.eu/schemas/edm/hasMet
:hasMet rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
        rdfs:label "Has Met"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> "edm:hasMet relates a resource with the objects or phenomena that have happened to or have happened together with the resource under consideration. We can abstractly think of history and the present as a series of “meetings” between people and other things in space-time. Therefore we name this relationship as the things the object “has met” in the course of its existence. These meetings are events in the proper sense, in which other people and things participate in any role."@en .


###  http://www.europeana.eu/schemas/edm/hasType
:hasType rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :isRelatedTo ;
         rdfs:domain [ rdf:type owl:Class ;
                       owl:unionOf ( :ProvidedCHO
                                     [ owl:intersectionOf ( <http://www.openarchives.org/ore/terms/Proxy>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty <http://www.openarchives.org/ore/terms/proxyFor> ;
                                                              owl:someValuesFrom :ProvidedCHO
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ]
                                   )
                     ] ;
         rdfs:range :NonInformationResource ;
         rdfs:label "Has Type"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> """This property relates a resource with the concepts it belongs to in a suitable
type system such as MIME or any thesaurus that captures categories of objects in a given field (e.g., the “Objects” facet in Getty’s Art and Architecture Thesaurus). It does not capture aboutness."""@en .


###  http://www.europeana.eu/schemas/edm/hasView
:hasView rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://www.openarchives.org/ore/terms/aggregates> ;
         rdfs:domain <http://www.openarchives.org/ore/terms/Aggregation> ;
         rdfs:range :WebResource ;
         rdfs:label "Has View"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> """This property relates a ORE aggregation about a CHO with a web resource
providing a view of that CHO. Examples of view are: a thumbnail, a textual
abstract and a table of contents. The ORE aggregation may be a Europeana
Aggregation, in which case the view is an object owned by Europeana (i.e., an instance of edm:EuropeanaObject) or an aggregation contributed by a content provider. In order to capture both these cases, the domain of edm:hasView is ore:Aggregation and its range is edm:WebResource"""@en .


###  http://www.europeana.eu/schemas/edm/incorporates
:incorporates rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isSimilarTo ;
              rdfs:label "Incorporates"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> """This property captures the use of some resource to add value to another
resource. Such resources may be nested, such as performing a theater play text, and then recording the performance, or creating an artful edition of a collection of poems or just aggregating various poems in an anthology. There may be no single part that contains ultimately the incorporated object, which may be dispersed in the presentation. Therefore, incorporated resources do in general not form proper parts. Incorporated resources are not part of the same resource, but are taken from other resources, and have an independent history. Therefore edm:incorporates is not a sub-property of dcterm:hasPart."""@en .


###  http://www.europeana.eu/schemas/edm/isAnnotationOf
:isAnnotationOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/subject> ,
                                   <http://www.cidoc-crm.org/rdfs/cidoc-crm#P67_refers_to> ;
                rdfs:domain :EuropeanaObject ;
                rdfs:range [ rdf:type owl:Class ;
                             owl:unionOf ( :ProvidedCHO
                                           [ owl:intersectionOf ( <http://www.openarchives.org/ore/terms/Proxy>
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty <http://www.openarchives.org/ore/terms/proxyFor> ;
                                                                    owl:someValuesFrom :ProvidedCHO
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ]
                                         )
                           ] ;
                rdfs:label "Is Annotation Of"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> """This property relates an annotation (a Europeana object) with the resource
that it annotates."""@en .


###  http://www.europeana.eu/schemas/edm/isDerivativeOf
:isDerivativeOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isSimilarTo ;
                rdfs:label "Is Derivative Of"@en ;
                <http://www.w3.org/2004/02/skos/core#definition> "This property captures a narrower notion of derivation than edm:isSimilarTo, in the sense that it relates a resource to another one, obtained by reworking, reducing, expanding, parts or the whole contents of the former, and possibly adding some minor parts. Versions have an even narrower meaning, in that it requires common identity between the related resources. Translations, summaries, abstractions etc. do not qualify as versions, but do qualify as derivatives."@en .


###  http://www.europeana.eu/schemas/edm/isNextInSequence
:isNextInSequence rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
                  rdfs:label "Is Next In Sequence Of"@en ;
                  <http://www.w3.org/2004/02/skos/core#definition> "edm:isNextInSequence relates two resources S and R that are ordered parts of the same resource A, and such that S comes immediately after R in the order created by their being parts of A."@en .


###  http://www.europeana.eu/schemas/edm/isRelatedTo
:isRelatedTo rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :ProvidedCHO
                                         [ owl:intersectionOf ( <http://www.openarchives.org/ore/terms/Proxy>
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty <http://www.openarchives.org/ore/terms/proxyFor> ;
                                                                  owl:someValuesFrom :ProvidedCHO
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ]
                                       )
                         ] ;
             rdfs:label "Is Related To"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> """edm:isRelatedTo is the most general contextual property in EDM. Contextual
properties have typically to do either with the things that have happened to or together with the object under consideration, or what the object refers to by its shape, form or features in a figural or encoded form. For sake of simplicity, we include in the contextual relationships also the scholarly classification, which may have either to do with the role and cultural connections of the object in the past, or its kind of structure, substance or contents as it can be verified at present."""@en .


###  http://www.europeana.eu/schemas/edm/isRepresentationOf
:isRepresentationOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/subject> ,
                                       <http://www.cidoc-crm.org/rdfs/cidoc-crm#P138_represents> ;
                    rdfs:domain :InformationResource ;
                    rdfs:label "Is Representation Of"@en ;
                    <http://www.w3.org/2004/02/skos/core#definition> "This property associates an information resource to the resource (if any) that it represents"@en .


###  http://www.europeana.eu/schemas/edm/isShownAt
:isShownAt rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasView ;
           rdfs:range :WebResource ;
           rdfs:label "Is Shown At"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "An unambiguous URL reference to the digital object on the provider’s web site in its full information context."@en .


###  http://www.europeana.eu/schemas/edm/isShownBy
:isShownBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasView ;
           rdfs:range :WebResource ;
           rdfs:label "Is Shown By"@en ;
           <http://www.w3.org/2004/02/skos/core#definition> "An unambiguous URL reference to the digital object on the provider’s web site in the best available resolution/quality."@en .


###  http://www.europeana.eu/schemas/edm/isSimilarTo
:isSimilarTo rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
             rdfs:label "Is Similar To"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "The most generic derivation property, covering also the case of questionable derivation. Is Similar To asserts that parts of the contents of one resource exhibit common features with respect to ideas, shapes, structures, colors, words, plots, topics with the contents of the related resource. Those common features may be attributed to a common origin or influence (in particular for derivation), but also to more generic cultural or psychological factors."@en .


###  http://www.europeana.eu/schemas/edm/isSuccessorOf
:isSuccessorOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :isSimilarTo ;
               rdfs:label "Is Successor Of"@en ;
               <http://www.w3.org/2004/02/skos/core#definition> "This property captures the relation between the continuation of a resource and that resource. This applies to a story, a serial, a journal etc. No content of the successor resource is identical or has a similar form with that of the precursor. The similarity is only in the context, subjects and figures of a plot. Successors typically form part of a common whole – such as a trilogy, a journal, etc."@en .


###  http://www.europeana.eu/schemas/edm/landingPage
:landingPage rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://www.openarchives.org/ore/terms/aggregates> ;
             rdfs:range :WebResource ;
             rdfs:label "Landing Page"@en ;
             <http://www.w3.org/2004/02/skos/core#definition> "This property captures the relation between a Europeana aggregation representing a cultural heritage object and a (reference) Web resource giving access to that object. Europeana provides the value for this property."@en .


###  http://www.europeana.eu/schemas/edm/object
:object rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :hasView ;
        rdfs:range :WebResource ;
        rdfs:label "Object"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> """The URL of a thumbnail representing the digital object or, if there is no such
thumbnail, the URL of the digital object in the best resolution available on the
web site of the data provider from which a thumbnail could be generated. This will often be the same URL as given in edm:isShownBy."""@en .


###  http://www.europeana.eu/schemas/edm/occurredAt
:occurredAt rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
            rdfs:domain :Event ;
            rdfs:range :TimeSpan ;
            rdfs:label "Occured At"@en ;
            <http://www.w3.org/2004/02/skos/core#definition> """This property associates an event to the smallest known time span that
overlaps with the occurrence of that event"""@en .


###  http://www.europeana.eu/schemas/edm/preview
:preview rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasView ;
         rdfs:range :WebResource ;
         rdfs:label "Preview"@en ;
         <http://www.w3.org/2004/02/skos/core#definition> "The URL of a thumbnail representing the digital object, generated by Europeana."@en .


###  http://www.europeana.eu/schemas/edm/provider
:provider rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasMet ;
          rdfs:range :Agent ;
          rdfs:label "Provider"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "The name or identifier of the organization who delivers data directly to an aggregation service (e.g. Europeana)"@en .


###  http://www.europeana.eu/schemas/edm/realizes
:realizes rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isRelatedTo ;
          rdf:type owl:AsymmetricProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :PhysicalThing ;
          rdfs:range :InformationResource ;
          rdfs:label "Realizes"@en ;
          <http://www.w3.org/2004/02/skos/core#definition> "This property describes a relation between a physical thing and the information resource that is contained in it, visible at it or otherwise carried by it, if applicable."@en .


###  http://www.europeana.eu/schemas/edm/rights
:rights rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:label "Europeana Rights"@en ;
        <http://www.w3.org/2004/02/skos/core#definition> """Information about copyright of the digital object as specified by isShownBy
and isShownAt"""@en .


###  http://www.europeana.eu/schemas/edm/uri
:uri rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf owl:topObjectProperty ;
     rdfs:label "Europeana URI"@en ;
     <http://www.w3.org/2004/02/skos/core#definition> "This is a tag created by a user through the Europeana interface."@en .


###  http://www.europeana.eu/schemas/edm/wasPresentAt
:wasPresentAt rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/dc/elements/1.1/relation> ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :Agent
                                          :InformationResource
                                          :PhysicalThing
                                        )
                          ] ;
              rdfs:range :Event ;
              rdfs:label "Was Present At"@en ;
              <http://www.w3.org/2004/02/skos/core#definition> "This property associates the people, things or information resources with an event at which they were present"@en .


###  http://www.openarchives.org/ore/terms/aggregates
<http://www.openarchives.org/ore/terms/aggregates> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf owl:topObjectProperty .


###  http://www.openarchives.org/ore/terms/proxyFor
<http://www.openarchives.org/ore/terms/proxyFor> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.europeana.eu/schemas/edm/begin
:begin rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty .


###  http://www.europeana.eu/schemas/edm/end
:end rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty .


###  http://www.europeana.eu/schemas/edm/type
:type rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:range [ rdf:type rdfs:Datatype ;
                   owl:oneOf [ rdf:type rdf:List ;
                               rdf:first "3D" ;
                               rdf:rest [ rdf:type rdf:List ;
                                          rdf:first "IMAGE" ;
                                          rdf:rest [ rdf:type rdf:List ;
                                                     rdf:first "SOUND" ;
                                                     rdf:rest [ rdf:type rdf:List ;
                                                                rdf:first "TEXT" ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "VIDEO" ;
                                                                           rdf:rest rdf:nil
                                                                         ]
                                                              ]
                                                   ]
                                        ]
                             ]
                 ] .


###  http://www.europeana.eu/schemas/edm/ugc
:ugc rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:range [ rdf:type rdfs:Datatype ;
                  owl:oneOf [ rdf:type rdf:List ;
                              rdf:first "TRUE" ;
                              rdf:rest rdf:nil
                            ]
                ] ;
     rdfs:label "UGC"@en ;
     <http://www.w3.org/2004/02/skos/core#definition> "This element is used to identify user generated content (also called user created content).  It should be applied to all digitised or born digital content contributed by the general public and collected by Europeana through a crowdsourcing initiative or project."@en .


