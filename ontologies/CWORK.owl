  <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/creativework/BlogPost">
    <rdfs:comment xml:lang="en-gb">This is the class of creative works that are associated with particular journalists or correspondents. The analysis, commentary and personal viewpoint of the creator is reflected on the blog post and the blog post is usually associated prominently with the name of the creator. BlogPost can have different types of formats.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:isDefinedBy rdf:resource="http://www.bbc.co.uk/ontologies/creativework/1.4"/>
    <rdfs:subClassOf rdf:resource="http://www.bbc.co.uk/ontologies/creativework/CreativeWork"/>
    <rdfs:label xml:lang="en-gb">BlogPost</rdfs:label>
    <ns1:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</ns1:deprecated>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/creativework/Category">
