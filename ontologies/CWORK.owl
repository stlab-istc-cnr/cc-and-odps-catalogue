    <rdfs:label xml:lang="en-gb">Category</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment xml:lang="en-gb">A class to represent different categorisations of creative works from different clients. This class is instantiated in the clients' application logic models.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.bbc.co.uk/ontologies/creativework/Audience">
    <rdfs:comment xml:lang="en-gb">Enumerated class of the audiences the BBC creates content for.</rdfs:comment>
